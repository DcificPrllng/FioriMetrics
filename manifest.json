{
	"_version": "1.5.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.pd.flp.ext",
		"type": "component",
		"applicationVersion": {
			"version": ""
		},
		"title": "{{plugin_title}}",
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZFIORI_ANALYTICS_SRV",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": []
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "OneWay",
					"refreshAfterChange": false,
					"defaultCountMode": "None"
				}
			}
		},		
		"contentDensities": {
			"compact": true,
			"cozy": false
		},
		"dependencies": {
			"minUI5Version": "1.38.1",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.1"
				},
				"sap.m": {
					"minVersion": "1.38.1"
				}
			}
		},
		"componentName": "com.pd.flp.ext"
	},
	"sap.flp": {
		"_version":"1.1.0",
		"type": "plugin",
		"config": {}
	}
}